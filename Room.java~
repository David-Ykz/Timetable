public class Room {
	private int roomId;
	private int roomNum;
    private String name;
    private String type;

	Room(int roomId, int roomNum, String name, String type) {
        this.roomNum = roomNum;
        this.name = name;
        this.type = type;
	}
	
	public int getRoomId() {
        return this.roomId;
    }
    
    public int getRoomNum() {
        return this.roomNum;
    }
}
